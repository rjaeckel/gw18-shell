          <wadl:doc><![CDATA[The user has not successfully authenticated]]></wadl:doc>
        </wadl:response>
        <wadl:response status="403">
          <wadl:doc><![CDATA[The user is not authorized to use this API]]></wadl:doc>
        </wadl:response>
        <wadl:response status="200">
          <wadl:doc><![CDATA[List of asynchronous job statuses.]]></wadl:doc>
          <wadl:representation mediaType="application/json"/>
